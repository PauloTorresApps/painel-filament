# ========================================
# CONFIGURAÇÃO DOCKER - SISTEMA DE ANÁLISE DE PROCESSOS
# ========================================

APP_NAME="Painel de Análise de Processos"
APP_ENV=production
APP_KEY=
APP_DEBUG=false
APP_TIMEZONE=America/Sao_Paulo
APP_URL=http://localhost:8000
APP_PORT=8000

APP_LOCALE=pt_BR
APP_FALLBACK_LOCALE=pt_BR
APP_FAKER_LOCALE=pt_BR

APP_MAINTENANCE_DRIVER=file
# APP_MAINTENANCE_STORE=database

BCRYPT_ROUNDS=12

LOG_CHANNEL=stack
LOG_STACK=single
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=info

# ========================================
# DATABASE - PostgreSQL
# ========================================
DB_CONNECTION=pgsql
DB_HOST=postgres
DB_PORT=5432
DB_DATABASE=painel
DB_USERNAME=painel_user
DB_PASSWORD=strongpassword123

# ========================================
# CACHE & SESSION - Redis
# ========================================
CACHE_STORE=redis
CACHE_PREFIX=painel_cache

SESSION_DRIVER=redis
SESSION_LIFETIME=120
SESSION_ENCRYPT=false
SESSION_PATH=/
SESSION_DOMAIN=null

REDIS_CLIENT=phpredis
REDIS_HOST=redis
REDIS_PASSWORD=null
REDIS_PORT=6379
REDIS_DB=0

# ========================================
# QUEUE - Redis
# ========================================
QUEUE_CONNECTION=redis

# ========================================
# FILESYSTEM
# ========================================
FILESYSTEM_DISK=local

# ========================================
# BROADCASTING
# ========================================
BROADCAST_CONNECTION=log

# ========================================
# MAIL
# ========================================
MAIL_MAILER=log
MAIL_HOST=127.0.0.1
MAIL_PORT=2525
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_ENCRYPTION=null
MAIL_FROM_ADDRESS="noreply@painel.local"
MAIL_FROM_NAME="${APP_NAME}"

# ========================================
# E-PROC WEBSERVICE
# ========================================
# URL do webservice MNI do e-Proc
URL_EPROC_WEBSERVICE=https://projudi.tjms.jus.br/projudi/intercomunicacao

# ========================================
# WEBSERVICE CNJ
# ========================================
# URL do webservice CNJ para consulta de classes e assuntos
URL_CNJ_WEBSERVICE=https://www.cnj.jus.br/sgt/sgt_ws.php

# ========================================
# AI CONFIGURATION
# ========================================
AI_DEFAULT_PROVIDER=gemini
AI_BATCH_SIZE=10
# Tamanho do lote para análise de documentos (evita erro 413)
# Recomendado: 5-10 para docs grandes, 15-20 médios, 30+ pequenos

# ========================================
# GEMINI AI CONFIGURATION
# ========================================
GEMINI_API_KEY=your_gemini_api_key_here
GEMINI_API_URL=https://generativelanguage.googleapis.com/v1beta/models
GEMINI_MODEL=gemini-1.5-flash
# Opções: gemini-1.5-pro (mais preciso, mais caro), gemini-1.5-flash (rápido, econômico)

# ========================================
# DEEPSEEK AI CONFIGURATION
# ========================================
DEEPSEEK_API_KEY=your_deepseek_api_key_here
DEEPSEEK_API_URL=https://api.deepseek.com/v1
DEEPSEEK_MODEL=deepseek-chat
# Opções: deepseek-chat (recomendado para análises), deepseek-coder (especializado em código)

# ========================================
# VITE (Frontend Assets)
# ========================================
VITE_APP_NAME="${APP_NAME}"

# ========================================
# DOCKER ESPECÍFICO
# ========================================
USER_ID=1000
GROUP_ID=1000
